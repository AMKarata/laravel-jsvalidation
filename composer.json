{
    "name": "proengsoft/laravel-jsvalidation",
    "type": "library",
    "description": "Validate forms with Javascript using your own FormRequest and Validators",
    "keywords": [
        "proengsoft",
        "laravel",
        "validator",
        "jQuery Validation"
    ],
    "homepage": "https://github.com/proengsoft/laravel-jsvalidation",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Moreno",
            "email": "a.moreno@proengsoft.com",
            "homepage": "http://www.proengsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Proengsoft\\JQueryValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proengsoft\\JQueryValidation\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
